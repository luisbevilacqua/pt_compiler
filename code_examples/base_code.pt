número mínimo := 0.
texto b := “Digite um número:”.

faça {
  imprima(b).
  número quantidade_de_numeros = entrada.
} enquanto(quantidade_de_numeros < mínimo).

número números_gerados := 0.

se(quantidade_de_numeros := 0) {
  imprima("Nenhum número a ser gerado").
}
senão {
  número número_gerado := 0.
  enquanto(números_gerados < quantidade_de_numeros){
    número_gerado := 3 + número_gerado * 5,5 / números_gerados - 1.
    imprima(número_gerado).

    números_gerados := números_gerados + 1.
  }
}
